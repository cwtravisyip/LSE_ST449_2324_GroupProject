   "source": [
    "## Formulate as a Tree\n",
    "At each node, it represents a subset of a schedule. Each increment in the depth of the node denotes a successive time slot.\n",
    "For simplicity, we assume that only **1 runway** is in operation and each slot is a duration of 5 minutes.\n",
    "In the first simple set up, we will assume the utility is the time delayed."
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 78,
   "metadata": {},
   "outputs": [],
   "source": [
    "class schedule_tree():\n",
    "    \"state should be in a dictionary format that has a time str as key `YY MM DD HH:mm`\"\n",
    "\n",
    "    def __init__(self, df_sorted,n_runway= 1,resume_hour = 23, resume_min= 0,time_slot_duration = 5):\n",
    "        self.df = df_sorted\n",
    "        self.move = df_sorted[\"code\"]\n",
    "        self.n_runway = n_runway\n",
    "        self.hour = resume_hour\n",
    "        self.min = resume_min\n",
    "        self.n = 0\n",
    "        self.time_slot_duration = time_slot_duration\n",
    "        self.date = self.df.loc[0,'time_sch'].day\n",
    "        self.month = self.df.loc[0,'time_sch'].month\n",
    "        self.year = self.df.loc[0,'time_sch'].year\n",
    "        self.schedule = dict() # to store the solution\n",
    "\n",
    "    def actions(self, state):\n",
    "        \"return a list of action, that is assigning the unassigned flights to the next time slot\"\n",
    "        all_flights = set(self.df['code'])\n",
    "        assigned_flight = set([flight for flight, util in state.values()])\n",
    "        unassigned_flight = all_flights - assigned_flight\n",
    "        # subset the flights that are not in time yet\n",
    "        year, month, date, hour, min = self.parse_state_time(state)\n",
    "        pd_timestamp = pd.to_datetime(f\"{year} {month} {date} {hour}:{min}\")\n",
    "        expected_flights = set(self.df[self.df['time_sch'] <= pd_timestamp]['code'])\n",
    "        # return the list of unassigned flight that has passed its schedule time\n",
    "        flight_to_assign = expected_flights.intersection(unassigned_flight)\n",
    "\n",
    "        return flight_to_assign # output as a set - iterable\n",
    "    \n",
    "    def parse_state_time(self,state):\n",
    "        \"Use to get the last item in the schedule and get parse the time of the next time slot\"\n",
    "        if len(state) <=0: \n",
    "            year = self.year\n",
    "            month = self.month\n",
    "            date = self.date\n",
    "            hour = self.hour\n",
    "            min = self.min\n",
    "\n",
    "        else:\n",
    "            timestr = list(state.keys())[-1]\n",
    "            year, month, date, time = timestr.split(\" \")\n",
    "            hour , min = time.split(\":\")\n",
    "            # get the current time\n",
    "            min = int(min)\n",
    "            min += self.time_slot_duration\n",
    "            if min // 60 <= 1:\n",
    "                hour = int(hour) + min // 60\n",
    "                min = min % 60\n",
    "                if hour // 24 <= 1:\n",
    "                    date = int(date) + hour // 24\n",
    "                    hour = hour % 24\n",
    "                \n",
    "        return year, month, date, hour,min\n",
    "\n",
    "    def result(self, state, move):\n",
    "        \"return the state in a form of dictionary that is the result of a given move\"\n",
    "        year, month, date, hour, min = self.parse_state_time(state)\n",
    "        key = f\"{year} {month} {date} {hour:02d}:{min:02d}\"\n",
    "        # get the utility\n",
    "        util = self.compute_util(move, year, month, date, hour, min)  \n",
    "\n",
    "        state[key] = (move, util)\n",
    "        return state\n",
    "    \n",
    "    def compute_util(self, move, year, month, date, hour, min):\n",
    "        move = move\n",
    "        if move is None:\n",
    "            return 0 \n",
    "        else:\n",
    "            time_sch_org = self.df.query(\"code == @move\")['time_sch'] # type pd Series\n",
    "            time_sch_new = pd.to_datetime(f\"{year} {month} {date} {hour}:{min}\")\n",
    "            # compute the time delayed\n",
    "            delay = time_sch_org - time_sch_new\n",
    "            delay = delay.reset_index(drop = True)[0].total_seconds()\n",
    "            return delay\n",
    "\n",
    "    def utility(self, state):\n",
    "        utils = [util for move, util in state.values()]\n",
    "        agg_util = sum(utils)\n",
    "        return agg_util\n",
    "\n",
    "    def terminal_test(self, state):\n",
    "        \"return True if the state is terminal\"\n",
    "        flight_assigned = [flight for flight, util in state.values() if flight is not None] \n",
    "        if len(flight_assigned) >= len(self.df):\n",
    "        # !!! this is not a robust way since time slot can be none\n",
    "            return True\n",
    "\n",
    "    def solve_schedule(self, solver):\n",
    "        n = 1\n",
    "        state = dict()\n",
    "        while True:\n",
    "            if self.terminal_test(self.schedule):\n",
    "                return (self.schedule, self.utility(self.schedule))\n",
    "            print(f\"Processing the {n}-th timeslot\")\n",
    "            # make a copy to prevent changes to the solution when searching\n",
    "            state = self.schedule.copy()\n",
    "            move = solver(schedule_prob = self, state = state) # solver is analogous to player object\n",
    "            # update the state\n",
    "            print(f\"{n}-th timeslot is assigned to {move}\")\n",
    "            # add the solution to the schedule\n",
    "            year, month, date, hour, min = self.parse_state_time(self.schedule)\n",
    "            key = f\"{year} {month} {date} {hour:02d}:{min:02d}\"\n",
    "            util = self.compute_util(move, year, month, date, hour, min)  \n",
    "            self.schedule[key] = (move,util)\n",
    "            print(f\"After processing for the best move, the state{self.schedule}\")\n",
    "            if self.terminal_test(self.schedule):\n",
    "                return (self.schedule, self.utility(self.schedule))\n",
    "            n+=1\n",
    "            \n"
   ]
