    "def max_solver(schedule_prob, state):\n",
    "\n",
    "    def max_value(state):\n",
    "        if schedule_prob.terminal_test(state):\n",
    "            # print(f\"Reached the terminal state {state}\")\n",
    "            return schedule_prob.utility(state)\n",
    "        v = -np.inf\n",
    "        for a in schedule_prob.actions(state):\n",
    "            v = max(v, max_value(schedule_prob.result(state,a)))\n",
    "        return v\n",
    "\n",
    "    # print(\"return the following move\")\n",
    "    actions = schedule_prob.actions(state)\n",
    "    if len(actions) < 1:\n",
    "        if schedule_prob.terminal_test(state):\n",
    "            # print(f\"Reached the terminal state {state}\")\n",
    "            return schedule_prob.utility(state)\n",
    "        else:\n",
    "            return None\n",
    "    else:\n",
    "        move = max(schedule_prob.actions(state), \n",
    "                key = lambda a: max_value(schedule_prob.result(state,a)))\n",
    "    \n",
    "    return move   "
   ]
